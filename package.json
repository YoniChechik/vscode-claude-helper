{
  "name": "claude-helper",
  "displayName": "Claude Helper",
  "description": "CLI tools for Claude Code - compare git refs, set terminal titles, notifications (VS Code Extension - CLI tool: uv tool install claude-helper)",
  "version": "1.1.2",
  "publisher": "YoniChechik",
  "author": {
    "name": "Yoni Chechik"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YoniChechik/vscode-claude-helper"
  },
  "bugs": {
    "url": "https://github.com/YoniChechik/vscode-claude-helper/issues"
  },
  "homepage": "https://github.com/YoniChechik/vscode-claude-helper#readme",
  "keywords": [
    "claude",
    "cli",
    "git",
    "compare",
    "terminal",
    "helper"
  ],
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "claude-helper.showLogs",
        "title": "Claude Helper: Show Logs"
      },
      {
        "command": "claude-helper.openClaudeTerminal",
        "title": "Claude Helper: Open Claude Terminal"
      }
    ]
  },
  "extensionDependencies": [
    "eamodio.gitlens"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.3"
  }
}
