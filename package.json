{
  "name": "gitlens-cli-bridge",
  "displayName": "GitLens CLI Bridge",
  "description": "Run GitLens compare commands from the CLI (VS Code Extension - CLI tool available via: uv tool install gitlens-cli-bridge)",
  "version": "1.0.0",
  "publisher": "YoniChechik",
  "author": {
    "name": "Yoni Chechik"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/YoniChechik/vscode-git-diff-extension"
  },
  "bugs": {
    "url": "https://github.com/YoniChechik/vscode-git-diff-extension/issues"
  },
  "homepage": "https://github.com/YoniChechik/vscode-git-diff-extension#readme",
  "keywords": [
    "gitlens",
    "cli",
    "git",
    "compare",
    "branches"
  ],
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gitlens-cli-bridge.showLogs",
        "title": "GitLens CLI Bridge: Show Logs"
      }
    ]
  },
  "extensionDependencies": [
    "eamodio.gitlens"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "typescript": "^4.9.3"
  }
}
